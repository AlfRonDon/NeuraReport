{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GeneratorSqlPack",
  "type": "object",
  "required": [
    "dialect",
    "script",
    "entrypoints",
    "params"
  ],
  "properties": {
    "dialect": {
      "type": "string"
    },
    "script": {
      "type": "string"
    },
    "entrypoints": {
      "type": "object",
      "required": ["header", "rows", "totals"],
      "properties": {
        "header": {
          "type": "string"
        },
        "rows": {
          "type": "string"
        },
        "totals": {
          "type": "string"
        }
      },
      "additionalProperties": {
        "type": "string"
      }
    },
    "params": {
      "type": "object",
      "required": ["required", "optional"],
      "properties": {
        "required": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "optional": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "additionalProperties": true
    },
    "notes": {
      "type": "string"
    }
  },
  "additionalProperties": true
}
