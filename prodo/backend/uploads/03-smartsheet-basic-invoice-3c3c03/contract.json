  },
  "date_columns": {
    "invoices": "issue_date"
  },
  "filters": {
    "optional": {}
  },
  "reshape_rules": [
    {
      "purpose": "Auto-generated reshape rule derived from mapping tokens.",
      "strategy": "SELECT",
      "columns": [
        {
          "as": "row_description",
          "from": [
            "invoice_items.description"
          ]
        },
        {
          "as": "row_total",
          "from": [
            "invoice_items.line_total"
          ]
        }
      ],
      "order_by": []
    }
  ],
  "row_computed": {
    "bill_to": {
      "op": "concat",
      "columns": [
        "customers.customer_name",
        "customers.billing_address",
        "customers.city",
        "customers.state",
        "customers.postal_code"
      ],
      "separator": "\n"
    },
    "ship_to": {
      "op": "concat",
      "columns": [
        "customers.customer_name",
        "customers.billing_address",
        "customers.city",
        "customers.state",
        "customers.postal_code"
      ],
      "separator": "\n"
    },
    "subtotal_less_discount": {
      "op": "subtract",
      "left": "invoices.subtotal",
      "right": "invoices.discount_amount"
    },
    "tax_rate": {
      "op": "divide",
      "numerator": "invoices.tax_amount",
      "denominator": "invoices.subtotal"
    },
    "shipping_handling": {
      "op": "multiply",
      "left": "invoices.invoice_number",
      "right": 0
    },
    "other": {
      "op": "multiply",
      "left": "invoices.invoice_number",
      "right": 0
    }
  },
  "totals_math": {},
  "totals": {},
  "formatters": {
    "invoice_date": "date(%Y-%m-%d)",
    "row_total": "number(2)",
